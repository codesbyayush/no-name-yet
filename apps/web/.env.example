VITE_SERVER_URL=https://localhost:8080

# API Configuration
VITE_API_BASE_URL=https://localhost:8080/api
VITE_API_TIMEOUT=30000
VITE_API_RETRIES=2

# Development Settings
VITE_ENABLE_API_LOGGING=true
VITE_ENABLE_DEVTOOLS=true

# Cache Configuration (in milliseconds)
VITE_DEFAULT_STALE_TIME=300000
VITE_AUTH_STALE_TIME=900000
VITE_SEARCH_STALE_TIME=30000

# Authentication Configuration
VITE_AUTH_TOKEN_KEY=auth_token
VITE_REFRESH_TOKEN_KEY=refresh_token
VITE_TOKEN_PREFIX=Bearer

# Production API URL (uncomment for production)
# VITE_PROD_API_URL=https://your-api-domain.com/api

# Development API URL (uncomment to override default)
# VITE_DEV_API_URL=http://localhost:3001/api
