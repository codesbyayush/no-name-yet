{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "types": ["@types/bun", "@cloudflare/workers-types"],
    "composite": true,
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*"],
  "tsc-alias": {
    "resolveFullPaths": true
  }
}
